<!--pages/shopMap/shopMap.wxml-->
<view class='body'>
<map
    id="map"
    longitude="{{longitude}}"
    latitude="{{latitude}}"
    markers="{{markers}}"
    scale="12"
    enable-zoom="false"
    bindmarkertap="markertap"
    bindcallouttap="callouttap"
    show-location
    style="width: 100%; height:100%"
  ></map>
</view>